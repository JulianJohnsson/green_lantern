<td>
  <%= transaction.category.emoji %>
</td>
<td>
  <%= transaction.description %>
</td>
<td class="d-none d-md-table-cell">
  <%= transaction.category.name %>
<td>
  <%= number_with_precision(transaction.carbone, precision: 2).to_s + " Kg" %>
</td>
<td class="d-none d-md-table-cell">
  <%= transaction.date %>
</td>
<td>
  <%= link_to edit_transaction_path(transaction, :month => params[:month], :category => transaction.category_id) do %>
    <i class="material-icons">more_vert</i>
  <% end %>
</td>


